<p id="notice"><%= notice %></p>


<h2><div id="brand-heading"><center>Our Coaches are "Real Coaches" <Br>Ready to transform your business</center></div></h2>	

<div class="grid-x align-top align-middle grid-margin-x grid-padding-x">

<% @coaches.each do |coach| %>
	<div class="cell grid-x small-4 self-align-middle">
		<div class="grid-y align-top align-middle">
			<div class="cell flex-child-auto"><%= link_to image_tag(rails_blob_path(coach.coachphoto)), coach %></div>
			<div class="cell flex-child-auto"><h5><%= coach.name%></h5></div>
		</div>
				
 	</div>

<% end%>

</div>
 <script>
      $(document).foundation();
    </script>
